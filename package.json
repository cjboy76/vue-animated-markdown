{
  "name": "vue-animated-markdown-monorepo",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.12.1",
  "scripts": {
    "dev": "pnpm --filter playground dev",
    "build:pkg": "pnpm --filter=!playground build",
    "build:playground": "pnpm --filter playground build",
    "build": "pnpm build:pkg && pnpm build:playground"
  }
}
